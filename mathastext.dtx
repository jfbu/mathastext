   \else\ifmt@itgreek\def\mt@tmp{it}
   \else\ifmt@frenchmath\def\mt@tmp{\csname mt@shape@\mt@version\endcsname}
   \else\def\mt@tmp{\csname mt@ltshape@\mt@version\endcsname}
   \fi\fi\fi
